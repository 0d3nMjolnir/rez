git:
    # If false, cancel a package release if there is no upstream branch.
    allow_no_upstream: false

    # The format used with 'git show' when retrieving the commit details.
    commit_details_format: "format:%C(bold)%ad (id: %h)%Creset%n%Cred%an: %Cgreen%s%Creset"

