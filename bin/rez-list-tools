#!/bin/bash
#
# List tools available in the current shell. Wrapped tools are suffixed with '*'.
#

for s in $REZ_META_TOOLS
do
	pkg=`echo $s | tr ':' ' ' | awk '{print $1}'`
	tools=`echo $s | tr ':' ' ' | tr ',' ' ' | awk '{$1=""; print $0}'`

	suffix=
	if [ "`echo $pkg | grep '^__wrapper_'`" != "" ]; then suffix='*'; fi
		
	for t in $tools
	do
		echo "$t""$suffix"
	done
done
